{
    "name": "pixilib/gaelo",
    "type": "project",
    "description": "GaelO Imaging platform for clinical trials",
    "version": "0.0.1",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "AGPL-3.0-only",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^2.0",
        "giggsey/libphonenumber-for-php": "^8.13",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "league/flysystem-azure-blob-storage": "^3.0",
        "league/flysystem-ftp": "^3.0",
        "league/flysystem-sftp-v3": "^3.0",
        "league/mime-type-detection": "^1.13",
        "phpoffice/phpspreadsheet": "^1.27",
        "sentry/sentry-laravel": "^4.4",
        "spatie/db-dumper": "^3.4",
        "staudenmeir/eloquent-has-many-deep": "^1.20"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "laravel/telescope": "^5.0",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^8.1",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Seeders\\Studies\\": "database/seeders/studies"
        },
        "classmap": [
            "database/seeders",
            "database/seeders/studies",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
